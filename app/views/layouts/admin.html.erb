<!DOCTYPE html>
<html>
<head>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
  <%= stylesheet_link_tag 'bootstrap','admin/icons_admin','admin/layout_admin','admin/corners_admin' %>
  <%= javascript_include_tag 'jquery','bootstrap','bootstrap-dropdown','rails','flash','dialog','tiny_mce/tiny_mce','application' %>
  <%= csrf_meta_tag %>
<title><%= $sitename %></title>
</head>
<body>
	<div id="header"><%= render :partial => 'layouts/admin_header' %></div>
	<%= render :partial => 'shared/admin/navbar' %>

  <div class="container-fluid">
    <div class="well" style="padding-top: 60px;">
      <%= yield(:toolbar) if yield(:toolbar).present? %>
      <%= yield %>
      </div>
  </div> <!-- /container -->
  <footer>
    <%= render :partial => 'layouts/admin_footer' %>
  </footer> 
</body>
</html>

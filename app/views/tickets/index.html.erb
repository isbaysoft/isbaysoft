<% form_for @ticket, tickets_url, :html => { :id => 'remoteform' } do |f| -%>
  <div class="content_box">
      <%= render :partial =>'form', :object => f -%>
  </div>
  <div class="clearboth">&nbsp;</div>
  <div class="content_box_bottom">
    <div class="right">
      <%= f.submit 'Отправить сообщение', :class => 'formsubmit', :style => 'margin: 10px;' %>
    </div>
      <div class="clearboth">&nbsp;</div>
  </div>
  
<% end %>
